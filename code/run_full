ipcluster stop;
cd amdtk;
python setup.py install;
cd ..;
echo "STARTING ENGINES";
ipcluster start --profile default -n 4 --daemonize;
sleep 10;
echo "ENGINES STARTED";
echo "RUNNING";
python -u run_amdtk_nc.py --bottom_plu_count=10 --n_epochs=1 --audio_dir=../audio/icicles --eval_dir=../audio/icicles --output_dir=output --batch-size=4 --decode | sed "s,\x1[Bb]\[[0-9;]*[a-zA-Z],,g" > debug.txt;
echo "DONE";
ipcluster stop;